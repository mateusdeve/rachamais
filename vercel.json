{
  "buildCommand": "npx expo export -p web",
  "outputDirectory": "dist",
  "functions": {
    "dist/server/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}